function demo(input){
    let degr = Number(input[0]);
    let timeOfDay = input[1];
    let outfit = ""; let shoes = "";
    switch(timeOfDay){
        case"Morning":
        if(degr >= 10 && degr <= 18){
            outfit = "Sweatshirt";
            shoes = "Sneakers";	
            }
        else if(degr > 18 && degr <= 24){
            outfit = "Shirt";
            shoes = "Moccasins";
        }
        else if(degr >= 25){
            outfit = "T-Shirt";
            shoes = "Sandals";
        }
        break;
        case"Afternoon":
        if(degr >= 10 && degr <= 18){
            outfit = "Shirt";
            shoes = "Moccasins";
            }
        else if(degr > 18 && degr <= 24){
            outfit = "T-Shirt";
            shoes = "Sandals";
        }
        else if(degr >= 25){
            outfit = "Swim Suit";
            shoes = "Barefoot";
        }
        break;
        case"Evening":
        if(degr >= 10 && degr <= 18){
            outfit = "Shirt";
            shoes = "Moccasins";
            }
        else if(degr > 18 && degr <= 24){
            outfit = "Shirt";
            shoes = "Moccasins";
        }
        else if(degr >= 25){
            outfit = "Shirt";
            shoes = "Moccasins";
        }
        break;
        default:
        break;
    }
    console.log(`It's ${degr} degrees, get your ${outfit} and ${shoes}.`);
}
